{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "outputDirectory": "frontend/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/build/$1"
      }
    ]
    {
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app"
}
  }
  
