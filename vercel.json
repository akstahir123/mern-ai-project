{
    "builds": [
      {
        "src": "frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "outputDirectory": "frontend/build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/build/$1"
      }
    ]
    "routes":[
        {"handle": "filesystem"},
        {"src": "/*", "dest": "/index.html"}
    ]
  }
  
